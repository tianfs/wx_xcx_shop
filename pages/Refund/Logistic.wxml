<view class='refund-list-wrp'>
  <view class="weui-cells weui-cells_after-title">
    <view class="weui-cell weui-cell_select">
      <view class="weui-cell__hd weui-cell__hd_in-select-after">
        <view class="weui-label">物流方式</view>
      </view>
      <view class="weui-cell__bd">
        <input bindinput="input_logistic_name" class='refund-bz-ipt' type='text' placeholder='请输入物流公司'></input>
      </view>
    </view>
  </view>
  <view class="weui-cells weui-cells_after-title">
    <view class="weui-cell weui-cell_select">
      <view class="weui-cell__hd weui-cell__hd_in-select-after select-arrow-null">
        <view  class="weui-label">物流单号</view>
      </view>
      <view class="weui-cell__bd">
        <input class='refund-bz-ipt' bindinput="input_logistic_number" type='text' placeholder='请输入物流单号'></input>
      </view>
    </view>
  </view>
</view>
<view class='refund-btn'>
  <button class="weui-btn" type="primary" bindtap="submit">提交</button>
</view>