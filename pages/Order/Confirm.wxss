@import '/weui.wxss';
.weui-cell__bd{
  font-size: 28rpx;
  color: #98948f;
}
.weui-cell__ft_in-access{
  font-size: 28rpx;
}
.weui-cell__ft_in-access::after{
  margin-top: -3px;
}
.c-order-border{
  height: 3px;
  display: block;
}
.c-order-border image{
  width: 100%;
  height: 3px;
  display: block;
}
.weui-panel{
  margin-top: 10rpx;
}
.c-bottom-wrp{
  padding-bottom: 120px;
}
.c-order-shop-icon{
  width: 29rpx;
  height: 29rpx;
  vertical-align: middle;
}
.c-order-shop-text{
  font-size: 28rpx;
  color: #000000;
  vertical-align: middle;
  margin-left: 10rpx;
}
.weui-media-box__bd_in-appmsg{
  overflow: hidden;
  display: inline-block;
}
.weui-media-box__desc{
  font-size: 28rpx;
  color: #363531;
}
.c-order-det{
  width: 150rpx;
  overflow: hidden;
  text-align: right;
}
.c-price1{
  font-size: 28rpx;
  color:#F16261;
}
.c-price2{
  font-size: 24rpx;
  color: #beb8b3;
  text-decoration: line-through;
}
.c-num{
  font-size: 24rpx;
  color: #838382;
}
.weui-cell__bd{
  text-align: right;
  font-size: 28rpx;
  color: #101010;
}
.weui-panel__hd::after{
  left: 0;
}
.weui-media-box::before{
  left: 0;
}
.weui-cell::before{
  left: 0;
}
.weui-cell__bd text{
  color:#F16261;
}
.weui-panel__ft{
  overflow: hidden;
}
.c-order-bottom{
  background: #ffffff;
  margin-top: 20rpx;
  border-top: 1rpx solid #d9d9d9;
  overflow: hidden;
  display: block;
}
.c-order-ipt{
  font-size: 28rpx;
  padding: 5px 15px;
  color: #aca8a3;
  border-bottom: 1rpx solid #d9d9d9;
}
.c-order-price-box{
  background: #ffffff;
  position: fixed;
  bottom: 0;
  left: 0;
  border-top: 1rpx solid #d9d9d9;
  z-index: 100;
}
.c-order-price{
  font-size: 28rpx;
  color: #6a6966;
  text-align: center;
  padding: 15px 0;
  width: 750rpx;
}
.c-order-price text{
  color:#F16261;
}
.c-order-price-btn text{
  text-align: center;
  font-size: 28rpx;
  display: block;
  padding: 15px;
  background: #F16261;
  color: #ffffff
}
.con-order-gg{
  font-size: 22rpx;
  color: #999999;
  margin-top: 5rpx;
}
.weui-media-box__title{
  font-size: 28rpx;
  color: #3e3e3e;
}
.det-user-name{
  float: left;
}
.det-phone-num{
  float: right;
}
.weui-media-box__desc{
  font-size: 28rpx;
}
.weui-media-box__hd_in-appmsg{
  width: 110rpx;
}
.order-add{
  width: 60rpx;
}